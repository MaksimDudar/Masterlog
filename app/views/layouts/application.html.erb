                    <a href="/" class="d-flex align-items-center my-2 my-lg-0 me-lg-auto text-white text-decoration-none">
                      <img src="11.png" alt="Master" width="90" height="60">
                    </a>

                    <ul class="nav col-12 col-lg-auto my-2 justify-content-center my-md-0 text-small fw-semibold">
                      <li>
                        <a href="#" class="nav-link active text-black">
                          <svg class="bi d-block mx-auto mb-1" width="24" height="8">
                          <img style='position:relative;left:60px;bottom:30px;' border="0" alt="W3Schools" src="7.png" width="60" height="60">
                            <use xlink:href="#house-fill"></use></svg>
